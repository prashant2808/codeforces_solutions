import java.util.*;
public class tetrahedron {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
 
        Set<Integer> set= new HashSet<>();
        for (int i=0;i < n;i++){
            set.add(sc.nextInt());
        }
        int ans=1;
        while(set.contains(ans))ans++;
 
        System.out.println(ans);
 
    }
}
